<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="io.husayn.paging_library_sample">

  <uses-permission android:name="android.permission.INTERNET" />

  <application
    android:name="paging.wrapper.app.PokemonApplication"
    android:allowBackup="true"
    android:icon="@drawable/pokedex"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="paging.wrapper.demo.MainActivity" android:exported="true">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name="paging.wrapper.demo.DetailActivity"
      android:label="@string/detail" />
  </application>

</manifest>